<!doctype html>
<html>
	<head>
		<title>
			<%= title%>
		</title>
	</head>
	<meta charset="utf-8">
	<link rel="stylesheet" type="text/css" href="bower_components/css-reset/reset.css">
	<link rel="stylesheet" type="text/css" href="bower_components/bootstrap/dist/css/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/app/music.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/app/style.css">
	<script type="text/javascript" data-main="src/app/include" src="bower_components/requirejs/require.js"></script>

	<style type="text/css">
		:root, body{
			width: 100%;
			height: 100%;
		}
	</style>
	<body>
		<%include cnknot%>
		<div class='bk'></div>
		<%include nav%>
		<div class="container">
			<div class="row">
				<div class='col-12'>
					<div class="ul-container">
						<ul id="me" class="music-ul">
				
						</ul>
					</div>
				</div>
			</div>
		</div>
		<nav class="navbar navbar-default navbar-fixed-bottom navbar-transparent">
		  <div class="container">
		  	<div class="row play-row">
		  		<div class='col-md-1 text-left play-col'>
		  			<div class='play-btn play-btn-backward'>
		  				<span class="glyphicon glyphicon-step-backward" aria-hidden="true" id='playbackward'></span>
		  			</div>
		  			<div class='play-btn play-btn-play'>
		  				<span class="glyphicon glyphicon-play" aria-hidden="true" id='playnow'></span>
		  			</div>
		  			<div class='play-btn play-btn-forward'>
		  				<span class="glyphicon glyphicon-step-forward" aria-hidden="true" id='playforward'></span>
		  			</div>
		  		</div>
		  		<div class='col-md-9 text-left play-col'>
		  			<div class="media">
					  <div class="media-left">
					    <a href="#">
					      <img class="media-object music-thumb" id="musicthumb">
					    </a>
					  </div>
					  <div class="media-body">
					    <h6 class="media-heading music-title" id='musictitle'></h6>
					    <div class="progress">
						  <div class="progress-bar progress-bar-warning progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;" id="nowProgress">
						  </div>
						  <div class="progress-bar progress-bar-info progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;" id="remainProgress">
						  </div>
						</div>
					  </div>
					  <div class='media-right'>
					  	<h6 class="media-heading music-title"></h6>
					  	<span id='playTime'>00:00/00:00</span>
					  </div>
					</div>
		  		</div>
		  		<div class='col-md-2 text-right play-col'>
		  			<div class='list-btn list-btn-volume'>
		  				<span class="glyphicon glyphicon-volume-off" aria-hidden="true"></span>
		  			</div>
		  			<div class='list-btn list-btn-volume'>
		  				<span class="glyphicon glyphicon-volume-down" aria-hidden="true"></span>
		  			</div>
		  			<div class='list-btn list-btn-volume'>
		  				<span class="glyphicon glyphicon-volume-up" aria-hidden="true"></span>
		  			</div>
		  			<div class='list-btn' id='listbtn'>
		  				<span class="glyphicon glyphicon-menu-hamburger" aria-hidden="true"></span>
		  			</div>
		  		</div>
		  	</div>
		  </div>
		</nav>
	</body>
</html>